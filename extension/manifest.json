{
    "manifest_version": 2,
    "name": "ClickOnce Helper",
    "short_name": "ClickOnce",
    "version": "1.0.0",
    "description": "Makes launching ClickOnce applications easier.",

    "icons": {
        "16": "icons/small-16.png",
        "32": "icons/small-32.png",
        "48": "icons/medium-48.png",
        "64": "icons/medium-64.png",
	"128": "icons/large-128.png"
    },

    "permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "plugins": [
        {
            "path": "native/NPClickOnce.dll",
            "public": true
        }
    ],
    "requirements": {
        "plugins": {
            "npapi": false
        }
    }
}